{
  "compilerOptions": {
    "jsx": "react",
    "module": "ESNext",
    "target": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "lib": ["ESNext", "DOM"],
    "sourceMap": false,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "baseUrl": "./src",
    "paths": {
      "@Components/*": ["components/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts"
  ],
"exclude": [
    "dist",
    "node_modules",
    "**/*.js"
  ],
}